{
  "name": "angularjs_app",
  "version": "0.1.0",
  "description": "AngularJS application prototyping",
  "repository": {
    "type": "git",
    "url": "https://github.com/jojanper/ng-app.git"
  },
  "bugs": {
    "url": "https://github.com/jojanper/ng-app/issues"
  },
  "scripts": {
    "prepare": "npm install && npm run prepare-bower",
    "prepare-bower": "bower install",
    "update-bower": "bower update",
    "build": "grunt dist",
    "test": "grunt unittest",
    "codestyle": "grunt code-checker",
    "docs": "grunt docs",
    "cibuild": "grunt cibuild --jenkins",
    "cibuild-travis": "grunt cibuild --travis",
    "dev-test": "npm run codestyle && npm test",
    "push-remote": "for r in $(git remote); do git push $r master; done",
    "release": "node nodejs/release",
    "posttest": "cat build/test_reports/coverage-unit/text.txt && cat build/test_reports/coverage-unit/text-summary.txt"
  },
  "author": "Juha Ojanpera juha.ojanpera@gmail.com",
  "license": "MIT",
